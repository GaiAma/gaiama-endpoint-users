{
  "release": {
    "npmPublish": false,
    "verifyConditions": [
      {
        "path": "@semantic-release/github"
      }
    ],
    "prepare": [
      {
        "path": "@semantic-release/git",
        "assets": ["package.json", "dist/**/*.{js,css}", "docs"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ],
    "publish": [
      "path": "@semantic-release/github",
      {
        "path": "@semantic-release/npm",
        "npmPublish": false,
      }
    ]
  }
}